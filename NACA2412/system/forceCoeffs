forceCoeffs1 
{
	type		forceCoeffs;

	libs		("libforces.so");

	writeControl	timeStep;
	timeInterval	10;

	log		yes;

	patches		(walls);
	rho		rhoInf;		// Indicates incompressible
	rhoInf		1.225;		// Redundant for incompressible
	liftDir		(0 1 0);
	dragDir		(1 0 0);
	CofR		(0 0 0); 	// Axle midpoint on ground
	pitchAxis	(0 0 1);
	magUInf		25.75;
	lRef		1;		// Wheelbase length
	Aref		1;		// Estimated
}

